{
  "name": "erp",
  "version": "1.0.0",
  "description": "A generator for locawebstyle.",
  "dependencies": {
	  "normalize-css" : ">= 2.1.3",
    "jquery" : ">= 2.0.1",
    "locawebstyle" : ">= 3.7.2"
  },
  "authors": [
    "emalherbi"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "exportsOverride": {
    "normalize-css": {
      "dist": "**.css"
    },
    "jquery": {
      "dist": "dist/**.js"
    },
    "locawebstyle": {
      "dist": "*"
    }
  }
}
